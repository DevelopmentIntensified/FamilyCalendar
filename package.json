		"@capacitor/core": "^7.0.1",
		"@capacitor/geolocation": "^7.1.1",
		"@capacitor/ios": "^7.0.1",
		"@ngneat/falso": "^7.3.0",
		"@oslojs/crypto": "^1.0.1",
